zendesk.trigger DP_Org_Test@s {
  title = "DP Org Test"
  active = true
  default = false
  actions = [
    {
      field = "current_tags"
      value = [
        zendesk.tag.instance.customer_engineering,
      ]
    },
    {
      field = "notification_user"
      value = [
        "dan.putman+zd@salto.io",
        "",
        "Something for DP!",
      ]
    },
    {
      field = "status"
      value = "pending"
    },
  ]
  conditions = {
    all = [
      {
        field = "role"
        operator = "is"
        value = "agent"
      },
      {
        field = "locale_id"
        operator = "is"
        value = zendesk.locale.instance.es
      },
      {
        field = "requester_id"
        operator = "is"
        value = "dan.putman+zd@salto.io"
      },
    ]
    any = [
      {
        field = "locale_id"
        operator = "is_not"
        value = zendesk.locale.instance.en_US@b
      },
    ]
  }
  description = "A new description for testing"
  raw_title = "DP Org Test"
  category_id = zendesk.trigger_category.instance.DP
  _alias = "DP Org Test"
}
