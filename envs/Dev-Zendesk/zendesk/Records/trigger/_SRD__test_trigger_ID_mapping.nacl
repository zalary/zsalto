zendesk.trigger _SRD__test_trigger_ID_mapping@jkssss {
  title = "(SRD) Product Access Update"
  active = true
  default = false
  actions = [
    {
      field = "notification_webhook"
      value = [
        zendesk.webhook.instance.SRD___update_jira___1@sbsssbs,
        '''
{
	"ticket" : {
		"custom_fields": [
			{
				"id": 9300587954327,
				"value": "{{ticket.ticket_field_${ zendesk.ticket_field.instance.SRD_Early_Product_Access_Type_tagger@bsssu }}}"
			}
		]
	}
}
''',
      ]
    },
  ]
  conditions = {
    all = [
      {
        field = "status"
        operator = "is"
        value = "open"
      },
      {
        field = "priority"
        operator = "is"
        value = "urgent"
      },
      {
        field = "comment_is_public"
        operator = "is"
        value = "false"
      },
      {
        field = zendesk.ticket_field.instance.SRD_Early_Product_Access_Type_tagger@bsssu
        operator = "present"
        value = ""
      },
    ]
  }
  raw_title = "(SRD) Product Access Update"
  category_id = zendesk.trigger_category.instance.Notifications
  _alias = "(SRD) Product Access Update"
}
