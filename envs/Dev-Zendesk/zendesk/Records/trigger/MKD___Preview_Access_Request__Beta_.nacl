      {
        field = zendesk.ticket_field.instance.MKD___Preview_Release_Type_tagger@sbsssu
        operator = "is"
        value = zendesk.ticket_field__custom_field_options.instance.MKD___Preview_Release_Type_tagger_sbsssu__beta_tester@uuuuuumuuu
      },
    ]
  }
  raw_title = "MKD - Preview Access Request (Beta)"
