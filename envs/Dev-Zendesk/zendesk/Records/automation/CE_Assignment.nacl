zendesk.automation CE_Assignment@s {
  title = "CE Assignment"
  active = true
  default = false
  actions = [
    {
      field = "assignee_id"
      value = "geoffrey.routledge@salto.io"
    },
  ]
  conditions = {
    all = [
      {
        field = "assignee_id"
        operator = "is"
        value = ""
      },
      {
        field = "NEW"
        operator = "greater_than"
        value = "24"
      },
      {
        field = "current_tags"
        operator = "includes"
        value = [
          zendesk.tag.instance.cusotmer_engineering,
        ]
      },
    ]
    any = [
      {
        field = zendesk.organization_field.instance.org_size
        operator = "greater_than"
        value = "300"
      },
      {
        field = zendesk.organization_field.instance.org_size
        operator = "less_than"
        value = "1000"
      },
    ]
  }
  raw_title = "CE Assignment"
  _alias = "CE Assignment"
}
