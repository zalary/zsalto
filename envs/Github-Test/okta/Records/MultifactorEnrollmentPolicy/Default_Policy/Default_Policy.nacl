  priority = 1
  system = true
  conditions = {
    people = {
      groups = {
        include = [
          okta.Group.instance.Everyone,
        ]
      }
    }
  }
  settings = {
    additionalProperties = {
      factors = {
        okta_password = {
          enroll = {
            self = "OPTIONAL"
          }
          consent = {
            type = "NONE"
          }
        }
      }
    }
  }
  type = "MFA_ENROLL"
  _alias = "Default Policy"
}
