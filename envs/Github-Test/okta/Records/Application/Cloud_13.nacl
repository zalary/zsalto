okta.Application Cloud_13@b {
  label = "Cloud-13"
  status = "ACTIVE"
  accessibility = {
    selfService = false
  }
  visibility = {
    autoSubmitToolbar = false
    hide = {
      iOS = false
      web = false
    }
  }
  signOnMode = "SAML_2_0"
  credentials = {
    userNameTemplate = {
      template = "\${source.email}"
      type = "BUILT_IN"
    }
  }
  settings = {
    notifications = {
      vpn = {
        network = {
          connection = "DISABLED"
        }
      }
    }
    manualProvisioning = false
    implicitAssignment = false
    signOn = {
      signOnMode = "SAML_2_0"
      defaultRelayState = "https://jacobl.humio.cloud"
      ssoAcsUrl = "https://jacobl.humio.cloud/api/v1/saml/acs"
      idpIssuer = "http://www.okta.com/\${org.externalKey}"
      audience = "https://jacobl.humio.cloud/api/v1/saml/metadata"
      recipient = "https://jacobl.humio.cloud/api/v1/saml/acs"
      destination = "https://jacobl.humio.cloud/api/v1/saml/acs"
      subjectNameIdTemplate = "\${user.userName}"
      subjectNameIdFormat = "urn:oasis:names:tc:SAML:1.1:nameid-format:emailAddress"
      responseSigned = true
      assertionSigned = true
      signatureAlgorithm = "RSA_SHA256"
      digestAlgorithm = "SHA256"
      honorForceAuthn = true
      authnContextClassRef = "urn:oasis:names:tc:SAML:2.0:ac:classes:PasswordProtectedTransport"
      requestCompressed = false
      allowMultipleAcsEndpoints = false
      samlSignedRequestEnabled = false
      slo = {
        enabled = false
      }
    }
  }
  _alias = "Cloud-13"
}
