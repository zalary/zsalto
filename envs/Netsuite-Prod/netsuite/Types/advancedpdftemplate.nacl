    _restriction = {
      regex = "^custtmpl[0-9a-z_]+"
    }
  }
  netsuite.advancedpdftemplate_standard standard {
    isAttribute = true
  }
  string title {
    _required = true
  }
  string description {
  }
  boolean displaysourcecode {
  }
  boolean preferred {
  }
  
  string trantype {
  }
  string printtype {
  }
  string savedsearch {
  }
  string recordtype {
  }
  netsuite.fileContent content {
    additionalFileSuffix = "xml"
  }
